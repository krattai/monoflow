@HAVE_GNOME_TRUE@install-data-hook: install-data-hook-omf
@HAVE_GNOME_FALSE@install-data-hook:

# At some point, it may be wise to change to something like this:
# scrollkeeper_localstate_dir = @SCROLLKEEPER_STATEDIR@

omf: omf_timestamp

omf_timestamp: $(omffile)
	-if test -n "$(omffile)"; then \
	  for file in $(omffile); do \
	    scrollkeeper-preinstall $(DESTDIR)$(helpdocdir)/$(docname).xml $(srcdir)/$$file $$file.out; \
	  done; \
	fi;
	touch omf_timestamp

install-data-hook-omf:
	$(mkinstalldirs) $(omf_dest_dir)
	-if test -n "$(omffile)"; then \
	  for file in $(omffile); do \
	    $(INSTALL_DATA) $$file.out $(omf_dest_dir)/$$file; \
	  done; \
	fi
	-scrollkeeper-update -p $(scrollkeeper_localstate_dir) -o $(omf_dest_dir)

uninstall-local-omf:
	-for file in $(srcdir)/*.omf; do \
	  basefile=`basename $$file`; \
	  rm -f $(omf_dest_dir)/$$basefile; \
	done
	-rmdir $(omf_dest_dir)
	-scrollkeeper-update -p $(scrollkeeper_localstate_dir)

clean-local-omf:
	-if test -n "$(omffile)"; then \
	  for file in $(omffile); do \
	    rm -f $$file.out; \
	  done; \
	fi

@HAVE_GNOME_TRUE@all: omf $(htmldoc) $(pdfdoc) $(psdoc)

@HAVE_GNOME_TRUE@install-data-local: install-data-xml \
@HAVE_GNOME_TRUE@	$(html_install) $(pdf_install) $(ps_install) install-examples

@HAVE_GNOME_TRUE@uninstall-local: uninstall-local-xml  \
@HAVE_GNOME_TRUE@	$(html_install) uninstall-pdf uninstall-ps uninstall-examples

@HAVE_GNOME_TRUE@clean-local: clean-local-xml \
@HAVE_GNOME_TRUE@	clean-html clean-ps clean-pdf

@HAVE_GNOME_FALSE@@HAVE_XSLTPROC_TRUE@all: $(htmldoc) $(pdfdoc) $(psdoc)

@HAVE_GNOME_FALSE@@HAVE_XSLTPROC_TRUE@install-data-local: $(html_install_data) \
@HAVE_GNOME_FALSE@@HAVE_XSLTPROC_TRUE@	$(html_install_ng) $(pdf_install) $(ps_install) install-examples

@HAVE_GNOME_FALSE@@HAVE_XSLTPROC_TRUE@uninstall-local: uninstall-local-html  \
@HAVE_GNOME_FALSE@@HAVE_XSLTPROC_TRUE@	uninstall-html-nognome uninstall-pdf uninstall-ps uninstall-examples
@HAVE_GNOME_FALSE@@HAVE_XSLTPROC_FALSE@all: $(htmldoc) $(pdfdoc) $(psdoc)

